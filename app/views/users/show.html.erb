<h2>All Users</h2>

<h3><%= session[:current_user_name] %></h3>
<table>
    <tr>
        <th>Event Title:</th>
        <th>Event Description:</th>
        <th>Event Date</th>
        <th>Event Location</th> 
    </tr>
    <% @event.each do |event|%>
    <tr>
        <td><%= event.title %></td>
        <td><%= event.description %></td>
        <td><%= event.date %></td>
        <td><%= event.location %></td>
    </tr>
    <% end %>
</table>

